{
  "name": "@autamedica/web-app",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "check-types": "tsc --noEmit",
    "build:cloudflare": "cd ../.. && pnpm build:packages:core && cd apps/web-app && next build",
    "deploy:cloudflare": "pnpm build:cloudflare && wrangler pages deploy .next --project-name=autamedica-web-app"
  },
  "dependencies": {
    "@autamedica/tailwind-config": "workspace:^",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.58.0",
    "framer-motion": "^12.23.12",
    "gsap": "^3.13.0",
    "next": "^15.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@autamedica/typescript-config": "workspace:*",
    "@cloudflare/next-on-pages": "^1.13.16",
    "@playwright/test": "^1.55.0",
    "@types/node": "^22.15.3",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "autoprefixer": "^10.0.0",
    "eslint": "^9.34.0",
    "postcss": "^8.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "5.9.2"
  }
}
