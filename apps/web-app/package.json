{
  "name": "@autamedica/web-app",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "prebuild:cloudflare": "rm -rf .next .open-next || true",
    "build:cloudflare": "pnpm run build && node ../../scripts/next/ensure-required-server-files.mjs --app web-app && opennextjs-cloudflare build --skip-next-build && node ../../scripts/next/patch-open-next.mjs",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@autamedica/tailwind-config": "workspace:^",
    "framer-motion": "^12.23.12",
    "gsap": "^3.13.0",
    "next": "15.5.4",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@autamedica/typescript-config": "workspace:*",
    "@cloudflare/next-on-pages": "^1.13.16",
    "@playwright/test": "^1.55.0",
    "@types/node": "^22.15.3",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "autoprefixer": "^10.0.0",
    "eslint": "^9.34.0",
    "postcss": "^8.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.9.2"
  }
}
