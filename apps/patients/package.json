{
  "name": "@autamedica/patients",
  "version": "0.1.0",
  "private": true,
  "description": "Portal personal para pacientes con acceso a resultados y prescripciones",
  "main": "src/app/page.tsx",
  "scripts": {
    "dev": "next dev -p 3003",
    "build": "next build",
    "prebuild:cloudflare": "rm -rf .next .open-next || true",
    "build:cloudflare": "pnpm run build && node scripts/ensure-required-server-files.mjs && opennextjs-cloudflare build --skip-next-build && node scripts/patch-open-next.mjs",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "deploy:cloudflare": "pnpm build:cloudflare && wrangler pages deploy .open-next --project-name=autamedica-patients",
    "cf:typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "dependencies": {
    "@autamedica/auth": "workspace:^",
    "@autamedica/config": "workspace:^",
    "@autamedica/hooks": "workspace:^",
    "@autamedica/session": "workspace:^",
    "@autamedica/shared": "workspace:^",
    "@autamedica/supabase-client": "workspace:^",
    "@autamedica/tailwind-config": "workspace:^",
    "@autamedica/telemedicine": "workspace:^",
    "@autamedica/types": "workspace:^",
    "@autamedica/ui": "workspace:^",
    "@autamedica/utils": "workspace:^",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.57.4",
    "clsx": "^2.1.1",
    "lucide-react": "^0.461.0",
    "next": "^15.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^1.9.1",
    "@playwright/test": "^1.42.1",
    "@types/node": "^22.10.1",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.5.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "vitest": "^1.3.1"
  }
}
