    "rootDir": "src",
    "declaration": true,
    "declarationMap": true,
    "composite": false,
    "incremental": false,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ES2022"
  },
  "include": ["src"],
  "exclude": ["dist", "node_modules"]
}