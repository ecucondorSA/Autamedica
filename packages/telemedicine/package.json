{
  "name": "@autamedica/telemedicine",
  "version": "0.2.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist *.tsbuildinfo && tsc --build tsconfig.build.json",
    "dev": "tsc --build --watch tsconfig.build.json",
    "clean": "rimraf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "check-types": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@autamedica/shared": "workspace:*",
    "@autamedica/types": "workspace:*",
    "lucide-react": "^0.544.0"
  },
  "devDependencies": {
    "@autamedica/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.14",
    "@types/react-dom": "^19.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@vitejs/plugin-react": "^5.0.4",
    "rimraf": "^5.0.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "peerDependenciesMeta": {
    "@supabase/supabase-js": {
      "optional": false
    }
  }
}
