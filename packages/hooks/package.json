{
  "name": "@autamedica/hooks",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "dev": "tsc --build --watch tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "check-types": "tsc --noEmit",
    "clean": "rimraf dist *.tsbuildinfo"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@autamedica/types": "workspace:*"
  },
  "devDependencies": {
    "@autamedica/typescript-config": "workspace:*",
    "@types/react": "^18.0.0 || ^19.0.0",
    "rimraf": "^5.0.0",
    "typescript": "5.9.2"
  }
}