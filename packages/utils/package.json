{
  "name": "@autamedica/utils",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "rimraf dist .tsbuildinfo && tsc --build tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist .tsbuildinfo"
  },
  "devDependencies": {
    "rimraf": "^5.0.0",
    "typescript": "^5.9.2"
  }
}
